<odoo>
    <data>
        <record model="ir.ui.view" id="meal_register_report_tree">
            <field name="name">Báo Cáo Suất Ăn</field>
            <field name="model">tigo.mealregister</field>
            <field name="arch" type="xml">
                <tree string="Báo Cáo Suất Ăn">
                    <field name="name"/>
                    <field name="register"/>
                    <field name="date"/>
                    <field name="meal_type"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
        <record id="act_meal_register_report_view" model="ir.actions.act_window">
            <field name="name">Báo Cáo Suất Ăn</field>
            <field name="res_model">tigo.mealregister</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="meal_register_report_tree"/>
            <field name="domain">[]</field>
        </record>
        <menuitem name="Báo Cáo"
                  id="root_report"
                  parent="z_base.meal_manegent_menu"
                  sequence="6"/>
        <menuitem id="meal_register_report"
                  name="Báo Cáo Suất Ăn"
                  parent="z_base.root_report"
                  action="act_meal_register_report_view"
                  sequence="1"/>
    </data>
</odoo>