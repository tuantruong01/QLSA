<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="_view_form" model="ir.ui.view">
            <field name="name">_view_form</field>
            <field name="model"></field>
            <field name="arch" type="xml">
                <form string="_form">
                    <sheet>
                        <group>
                            <field name="name"/>

                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="_view_tree" model="ir.ui.view">
            <field name="name">_view_tree</field>
            <field name="model"></field>
            <field name="arch" type="xml">
                <tree string="_tree">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="_action" model="ir.actions.act_window">
            <field name="name"></field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model"></field>
            <field name="view_mode">tree,form</field>
        </record>
        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="UNIQUE_ID_categ" name="" parent="" action="" sequence=""/>
    </data>
</odoo>